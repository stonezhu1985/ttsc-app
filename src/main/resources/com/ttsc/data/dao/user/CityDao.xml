<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ttsc.data.dao.user.CityDao">
    
     <select id="findCityInfo"  resultType="City">
        select 
        	serialId id,cityName cityName
        from  
        	fch_city 
        where cityUpIdNum = #{id}
    </select>

</mapper>